{
  "name": "vue-flow-vis",
  "version": "0.1.0",
  "description": "Real-time monitoring of Vue 3 component renders and reactive dependencies",
  "keywords": [
    "vue",
    "vue3",
    "monitoring",
    "debugging",
    "development",
    "performance",
    "typescript",
    "devtools"
  ],
  "author": "Milorad FilipoviÄ‡ <milorad.filipovic19@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/MiloradFilipovic/vue-flow-vis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiloradFilipovic/vue-flow-vis.git"
  },
  "bugs": {
    "url": "https://github.com/MiloradFilipovic/vue-flow-vis/issues"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "version": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepare": "husky install"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/index.cjs",
      "limit": "3 KB"
    }
  ],
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.3.0",
    "husky": "^8.0.0",
    "prettier": "^3.6.2",
    "size-limit": "^11.2.0",
    "tsup": "^8.5.0",
    "vue": "^3.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
